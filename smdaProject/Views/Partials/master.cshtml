<script>
    function openPDF(pdfUrl, pdfName) {
        var iframe = document.createElement('iframe');
        iframe.src = pdfUrl;
        iframe.style.width = '100%';
        iframe.style.height = '500px'; // Set the height according to your needs
        iframe.style.border = 'none'; // Optional: Remove iframe border
        iframe.setAttribute('title', pdfName); // Optional: Set title for accessibility
        document.body.appendChild(iframe);
    }
